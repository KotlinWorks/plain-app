<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string" />
    <attr name="isRequired" format="boolean" />

    <declare-styleable name="ListItemView">
        <attr name="keyText" format="string" />
        <attr name="keyTextColor" format="color" />
        <attr name="valueText" format="string" />
        <attr name="showMore" format="boolean" />
        <attr name="isRequired" />
        <attr name="requiredErrorText" format="string" />
    </declare-styleable>

    <declare-styleable name="NoDataView">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="LoadingButtonView">
        <attr name="text" />
        <attr name="loadingText" format="string" />
        <attr name="buttonType" format="enum">
            <enum name="normal" value="0" />
            <enum name="danger" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MockPlaceHolder">
        <attr name="repeat" format="integer" />
        <attr name="layoutRes" format="integer" />
    </declare-styleable>

    <declare-styleable name="SearchView">
        <attr name="hintText" format="string" />
    </declare-styleable>

    <declare-styleable name="TextInputView">
        <attr name="hint" format="string" />
        <attr name="isRequired" />
        <attr name="placeholderText" format="string" />
        <attr name="helperText" format="string" />
        <attr name="lines" format="integer" />
        <attr name="endIconMode" />
        <attr name="endIconDrawable" />
        <attr name="android:inputType" />
    </declare-styleable>
</resources>